{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "availity-reactstrap-validation": "^2.5.4",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "express-session": "^1.16.1",
    "express-validator": "^6.0.1",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "mdbreact": "^4.16.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "normalize.css": "^8.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-animated-slider": "^1.1.4",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-bootstrap-validation": "^0.1.11",
    "react-dom": "^16.8.6",
    "react-password-mask": "^3.3.1",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.0",
    "sequelize": "^6.0.0",
    "yarn": "^1.16.0"
  }
}
